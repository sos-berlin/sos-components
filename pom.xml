  <build>
    <plugins>
      <!-- configuration of the release properties -->
      <plugin>
        <artifactId>maven-release-plugin</artifactId>
        <configuration>
          <!-- for release:prepare -->
          <tagNameFormat>v@{project.version}</tagNameFormat>
          <updateVersionsToSnapshot>true</updateVersionsToSnapshot>
          <autoVersionSubmodules>true</autoVersionSubmodules>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.10.1</version>
        <configuration>
          <source>17</source>
          <target>17</target>
          <!-- <optimize>true</optimize> -->
          <showDeprecation>true</showDeprecation>
          <showWarnings>true</showWarnings>
        </configuration>
      </plugin>
        <plugin>
          <groupId>org.cyclonedx</groupId>
          <artifactId>cyclonedx-maven-plugin</artifactId>
          <version>2.7.9</version>
          <inherited>false</inherited>
          <executions>
            <execution>
              <phase>package</phase>
              <goals>
                <goal>makeAggregateBom</goal>
              </goals>
            </execution>
        </executions>
        <configuration>
          <projectType>library</projectType>
          <schemaVersion>1.4</schemaVersion>
          <includeBomSerialNumber>true</includeBomSerialNumber>
          <includeCompileScope>true</includeCompileScope>
          <includeProvidedScope>true</includeProvidedScope>
          <includeRuntimeScope>true</includeRuntimeScope>
          <includeSystemScope>true</includeSystemScope>
          <includeTestScope>false</includeTestScope>
          <includeLicenseText>false</includeLicenseText>
          <excludeArtifactId>sos-annotator</excludeArtifactId>
          <outputReactorProjects>false</outputReactorProjects>
          <outputFormat>json</outputFormat>
          <outputName>sos-components-sbom</outputName>
        </configuration>
      </plugin>
    </plugins>
