<?xml version="1.0" encoding="iso-8859-1"?>
<param
	xmlns="http://www.sos-berlin.com/schema/js7_job_documentation_v1.1"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xhtml="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	name="target_file"
	required="true">
	
    <note>
        <title>Target file or directory</title>
		<p>Target file or directory</p>
		<p>
			If the
			<paramref>target_file</paramref>
			argument is not specified then the directory specified with the
			<paramref>source_file</paramref>
			argument will be used as the target.
		</p>
	</note>
    <xi:include href="./note_param_mask_substitution.xml" parse="xml"/>
</param>

 