{
  "title": "jobscheduler",
  "id": "schemas/jobscheduler/jobscheduler",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.jobscheduler.JobScheduler",
  "type": "object",
  "oneOf": [
    {
      "description": "if state.text=unreachable",
      "required": [
        "id",
        "surveyDate",
        "jobschedulerId",
        "url",
        "state"
      ]
    },
    {
      "required": [
        "id",
        "surveyDate",
        "jobschedulerId",
        "host",
        "url",
        "state",
        "version",
    	"os",
    	"timeZone",
    	"clusterType",
        "startedAt"
      ]
    }
  ],
  "properties": {
    "id": {
      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "surveyDate": {
      "$ref": "../common/surveyDate_v-schema.json"
    },
    "jobschedulerId": {
      "type": "string"
    },
    "host": {
      "type": "string"
    },
    "state": {
      "$ref": "state-schema.json"
    },
    "url": {
      "type": "string"
    },
    "clusterType": {
      "$ref": "clusterMemberType-schema.json"
    },
    "startedAt": {
      "$ref": "../common/timestamp-schema.json"
    },
    "version": {
      "type": "string"
    },
    "os": {
      "$ref": "os-schema.json"
    },
    "timeZone": {
      "type": "string"
    }
  }
}
