{
  "title": "dailyPlannedOrderItem",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "javaType": "com.sos.joc.model.dailyplan.PlannedOrderItem",
  "oneOf": [
    {
      "required": [
        "surveyDate",
        "workflow",
        "orderId",
        "plannedStartTime",
        "expectedEndTime",
        "startMode"
      ]
    }
  ],
  "properties": {
    "surveyDate": {
      "$ref": "../../../joc/schemas/common/timestamp-schema.json"
    },
    "workflow": {
      "$ref": "../../../joc/schemas/common/path-schema.json"
    },
    "orderId": {
      "type": "string"
    },
    "plannedStartTime": {
      "$ref": "../../../joc/schemas/common/timestamp-schema.json"
    },
    "expectedEndTime": {
      "$ref": "../../../joc/schemas/common/timestamp-schema.json"
    },
    "startTime": {
      "$ref": "../../../joc/schemas/common/timestamp-schema.json"
    },
    "endTime": {
      "$ref": "../../../joc/schemas/common/timestamp-schema.json"
    },
    "historyId": {
      "type": "string"
    },
    "node": {
      "description": "only for orders",
      "type": "string"
    },
    "exitCode": {
      "description": "only for standalone jobs",
      "$ref": "../../../joc/schemas/common/nonNegativeInteger-schema.json"
    },
    "error": {
      "$ref": "../../../joc/schemas/common/error-schema.json"
    },
    "startMode": {
      "description": "0=single_start, 1=start_start_repeat",
      "type": "integer",
      "default": 0
    },
    "period": {
      "description": "undefined for startMode=0",
      "required": [
        "begin",
        "end",
        "repeat"
      ],
      "type": "object",
      "properties": {
        "begin": {
          "$ref": "../../../joc/schemas/common/timestamp-schema.json"
        },
        "end": {
          "$ref": "../../../joc/schemas/common/timestamp-schema.json"
        },
        "repeat": {
          "$ref": "../../../joc/schemas/common/nonNegativeLong-schema.json"
        }
      }

    },
    "late": {
      "type": "boolean"
    },
    "state": {
      "$ref": "plannedOrderState-schema.json"
    }
  }
}
