{
	"title": "Order Template",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.webservices.order.initiator.model.OrderTemplate",
	"type": "object",
	"description": "The order template for scheduling orders to JobScheduler",
 	"javaInterfaces" : [
    	"com.sos.joc.model.common.IJSObject"
  	],
  	"required": [
		"controllerId",
		"orderTemplatePath",
		"templateId",
 
		"workflowPath"
	],
	"properties": {
		"controllerId": {
			"type": "string"
		},
		"orderTemplatePath": {
			"type": "string"
		},
		"workflowPath": {
			"$ref": "../../../joc/schemas/common/path-schema.json"
		},
		"submitOrderToControllerWhenPlanned": {
			"type": "boolean"
		},
		"planOrderAutomatically": {
			"type": "boolean"
		},
		"calendars": {
			"$ref": "../calendar/calendars-list-schema.json"
		},
		"nonWorkingCalendars": {
			"$ref": "../calendar/calendars-list-non-working-schema.json"
		},
		"variables": {
			"$ref": "../common/nameValuePairs-schema.json"
		}
	}
}
 
