{
	"title": "add order",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.webservices.order.initiator.model.OrderTemplate",
	"type": "object",
	"description": "The order template for scheduling orders to JobScheduler",
	"required": [
		"jobschedulerId",
		"orderTemplateName",
		"templateId",
		"workflowPath"
	],
	"properties": {
		"jobschedulerId": {
			"type": "string"
		},
		"orderTemplateName": {
			"type": "string"
		},
		"templateId": {
			"$ref": "../../../joc/schemas/common/nonNegativeLong-schema.json"
		},
		"workflowPath": {
			"type": "string"
		},
		"submit_order_to_controller_when_planned": {
			"type": "boolean"
		},
		"plan_order_automatically": {
			"type": "boolean"
		},
 		"calendars": {
			"$ref": "../calendar/calendars-list-schema.json"
		},
		"nonWorkingCalendars": {
			"$ref": "../calendar/calendars-list-non-working-schema.json"
		},
		"variables": {
			"$ref": "../common/nameValuePairs-schema.json"
		}
	}
}
 
