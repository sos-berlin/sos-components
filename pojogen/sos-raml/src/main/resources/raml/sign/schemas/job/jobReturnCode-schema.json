{
  "title": "job return code meaning",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.sign.model.job.JobReturnCode",
  "type": "object",
  "oneOf": [
    { "required": [ "success" ] }, 
    { "required": [ "failure" ] }
  ],
  "properties": {
    "success": {
    	"oneOf": [
    		{
      			"type": "array",
      			"minItems": 1,
      			"items": {
        			"type": "integer"
      			}
      		},
      		{
      			"type": "string",
      			"pattern": "^-?[0-9]+(\\.\\.-?[0-9]+)?(,-?[0-9]+(\\.\\.-?[0-9]+)?)*$"
      		}
      	]
    },
    "failure": {
      	"oneOf": [
    		{
      			"type": "array",
      			"minItems": 1,
      			"items": {
        			"type": "integer"
      			}
      		},
      		{
      			"type": "string",
      			"pattern": "^-?[0-9]+(\\.\\.-?[0-9]+)?(,-?[0-9]+(\\.\\.-?[0-9]+)?)*$"
      		}
      	]
    }
  },
  "additionalProperties": false
}