{
  "title": "ReadNotice",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.sign.model.instruction.ReadNotice",
  "type": "object",
  "required": [
  	"boardPath"
  ],
  "extends" : {
    "$ref": "instruction-schema.json"
  },
  
