    "boardPath"
  ],
  "properties": {
    "boardPath": {
    	"alias": "boardName",
    	"type": "string",
    	"pattern": "^[^<>]*$",
  		"maxLength": 255,
  		"minLength": 1
    }
  },
  "additionalProperties": false
}
