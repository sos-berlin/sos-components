{
	"title": "Cancel Order",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.jobscheduler.model.command.CancelOrder",
	"type": "object",
	"extends": {
		"javaType": "Command"
	},
	"required": [
		"orderId"
	],
	"propertyOrder": [
    	"TYPE",
    	"orderId",
    	"mode"
  	],
  	"properties": {
		"orderId": {
			"type": "string"
		},
		"mode": {
			"type": "object",
			"javaType": "com.sos.jobscheduler.model.order.OrderMode",
			"properties": {
				"TYPE": {
					"$ref": "../order/orderModeType-schema.json"
				},
				"kill": {
				  "type": "object",
				  "properties": {
				    "signal": {
				      "type": "string",
				      "enum": [
				        "SIGTERM",
				        "SIGKILL"
				      ],
				      "default": "SIGTERM"
				    },
				    "workflowPosition": {
				      "$ref": "../workflow/workflowPosition-schema.json"
				    }
				  }
				}
			},
			"default": "new com.sos.jobscheduler.model.order.OrderMode()",
			"additionalProperties": false
		}
	},
	"additionalProperties": false
}