{
  "title": "overview",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.jobscheduler.model.cluster.ClusterState",
  "type": "object",
  "required": [
  	"TYPE"
  ],
  "properties": {
    "TYPE": {
      "javaType": "com.sos.jobscheduler.model.cluster.ClusterType",
      "type": "string",
      "enum": [
        "Empty",
        "NodesAppointed",
        "PreparedToBeCoupled",
        "Coupled",
        "PassiveLost",
        "SwitchedOver",
        "FailedOver",
        "CoupledActiveShutDown" 
      ]
    },
    "setting": {
      "type": "object",
      "javaType": "com.sos.jobscheduler.model.cluster.ClusterSetting",
      "properties": {
        "idToUri": {
          "$ref": "idToUri-schema.json"
        },
        "activeId": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
