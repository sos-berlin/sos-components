{
	"title": "release",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.inventory.release.ReleaseFilter",
	"type": "object",
	"oneOf" : [
	  { "required": [ "controllerIds", "update" ] },
	  { "required": [ "delete" ] }
	],
	"properties": {
		"controllerIds": {
			"type": "array",
			"items": {
				"$ref": "../../common/string100-schema.json"
			}
		},
		"update": {
			"type": "array",
			"items": {
				"type": "object",
        		"javaType": "com.sos.joc.model.inventory.release.ReleaseUpdate",
        		"properties": {
          			"configurationId": {
            			"$ref": "../../common/nonNegativeLong-schema.json"
          			},
          			"releaseId": {
            			"$ref": "../../common/nonNegativeLong-schema.json"
          			}
        		}
			}
		},
		"delete": {
      		"type": "array",
      		"items": {
        		"type": "object",
        		"javaType": "com.sos.joc.model.inventory.release.ReleaseDelete",
        		"properties": {
          			"configurationId": {
            			"$ref": "../../common/nonNegativeLong-schema.json"
          			}
        		}
      		}
    	}
  	}
}
