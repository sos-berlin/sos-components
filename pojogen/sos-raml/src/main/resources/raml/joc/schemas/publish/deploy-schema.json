{
	"title": "deploy",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.DeployFilter",
	"type": "object",
	"required": [
		"controllers"
	],
	"properties": {
		"controllers": {
			"type": "array",
			"items": {
			  "type": "object",
			  "javaType": "com.sos.joc.model.publish.Controller",
			  "required": [
			    "controller"
			  ],
			  "properties": {
          "controller": {
            "$ref": "../common/string255-schema.json"
          }
			  }
			}
		},
		"update": {
			"type": "array",
			"items": {
			  "type": "object",
        "javaType": "com.sos.joc.model.publish.DeployUpdate",
        "properties": {
          "configurationId": {
            "$ref": "../common/nonNegativeLong-schema.json"
          },
          "deploymentId": {
            "$ref": "../common/nonNegativeLong-schema.json"
          }
        }
			}
		},
		"delete": {
      "type": "array",
      "items": {
        "type": "object",
        "javaType": "com.sos.joc.model.publish.DeployDelete",
        "properties": {
          "configurationId": {
            "$ref": "../common/nonNegativeLong-schema.json"
          }
        }
      }
    },
    "auditLog": {
      "$ref": "../audit/auditParams-schema.json"
    }
  }
}
