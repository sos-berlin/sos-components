{
	"title": "JS Object configuration",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.JSObject",
	"type": "object",
	"properties": {
		"id": {
			"$ref": "../common/nonNegativeLong-schema.json"
		},
    "editAccount": {
      "type": "string"
    },
		"publishAccount": {
			"type": "string"
		},
		"path": {
			"$ref": "../common/path-schema.json"
		},
		"objectType": {
			"type": "object",
			"javaType": "com.sos.jobscheduler.model.deploy.DeployType"
		},
    "content": {
      "type": "object",
      "javaType": "com.sos.joc.model.publish.IJSObject"
    },
    "signedContent": {
      "type": "string"
    },
		"version": {
			"type": "string"
		},
		"parentVersion": {
			"type": "string"
		},
		"comment": {
			"type": "string"
		},
		"modified": {
			"$ref": "../common/timestamp-schema.json"
		}
	}
}
