{
  "title": "signed object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.publish.SignedObject",
  "type": "object",
  "required": [
  	"string",
  	"signature"
  ],
  "properties": {
  	"string": {
      "type": "string"
    },
    "signature": {
      "type": "object",
      "javaType": "com.sos.joc.model.publish.Signature",
      "required": [
  	    "TYPE",
  	    "signatureString"
      ],
      "properties": {
        "TYPE": {
          "javaType": "com.sos.jobscheduler.model.deploy.SignatureType",
          "type": "string",
          "enum": [
            "PGP",
            "Silly"
          ],
          "default": "PGP"
        },
        "signatureString": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
