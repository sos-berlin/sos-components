{
	"title": "re-deploy",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.RedeployFilter",
	"type": "object",
	"required": [
		"controllerId",
		"folder"
	],
	"properties": {
		"controllerId": {
			"$ref": "../common/string255-schema.json"
		},
		"folder": {
			"$ref": "../common/string255-schema.json"
		},
		"excludes": {
			"type": "array",
			"items": {
				"type": "object",
				"javaType": "com.sos.joc.model.publish.ExcludeConfiguration",
				"required": [
					"path",
					"deployType"
				],
				"properties": {
					"path": {
						"$ref": "../common/string255-schema.json"
					},
					"deployType": {
						"$ref": "../inventory/common/configurationType-schema.json"
					}
				}
			}
		},
		"auditLog": {
			"$ref": "../audit/auditParams-schema.json"
		}
	}
}
