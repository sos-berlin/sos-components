{
	"title": "ExportFilter",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.ExportFilter",
	"type": "object",
  "anyOf": [{
    "required": [
        "forSigning",
        "deployables"
      ]
    }, {
      "required": [
        "forSigning",
        "releasables"
      ]
    }
  ],
 	"properties": {
    "forSigning": {
      "description": "decides if the export is meant for signing the exported objects [default=false]",
      "type" : "boolean"
    },
    "controllerId": {
      "$ref": "../common/string255-schema.json"
    },
    "deployables": {
        "$ref": "exportDeployables-schema.json"
    },
    "releasables": {
        "$ref": "exportReleasables-schema.json"
    },
    "auditLog": {
      "$ref": "../audit/auditParams-schema.json"
    }
	}
}
