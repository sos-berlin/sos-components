{
	"title": "Assigned Calendars",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "array",
	"minItems": 1,
	"items": {
		"javaType": "com.sos.webservices.order.initiator.model.AssignedCalendars",
		"type": "object",
		"required": [
			"calendarPath",
			"periods"
		],
		"properties": {
			"calendarPath": {
				"$ref": "../common/path-schema.json"
			},
			"timeZone": {
				"type": "string",
				"default": "Etc/UTC"
			},
			"includes": {
				"$ref": "frequencies-schema.json"
			},
			"excludes": {
				"$ref": "frequencies-schema.json"
			},
			"periods": {
				"type": "array",
				"items": {
					"$ref": "period-schema.json"
				}
			}
		}
	}
}
