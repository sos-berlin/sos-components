{
  "title": "jobscheduler masters",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.jobscheduler.Masters",
  "type": "object",
  "required": [
    "deliveryDate",
    "masters"
  ],
  "properties": {
    "deliveryDate": {
      "$ref": "../common/deliveryDate-schema.json"
    },
    "masters": {
      "type": "array",
      "items": {
        "$ref": "jobscheduler_v-schema.json"
      }
    },
    "clusterState": {
      "javaType": "com.sos.joc.model.jobscheduler.ClusterState",
      "type": "object",
      "required": [
        "severity",
        "_text"
      ],
      "properties": {
        "severity": {
          "type": "integer"
        },
        "_text": {
          "type": "string"
        }
      }
    }
  }
}
