{
  "title": "job",
  "id": "schemas/instruction/job",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.jobscheduler.model.instruction.Job",
  "javaInterfaces" : [
    "IInstruction"
  ],
  "type": "object",
  "required": [
  	"TYPE",
  	"jobPath"
  ],
  "extends" : {
    "$ref": "instruction-schema.json"
  },
  "properties": {
  	"TYPE": {
      "javaType": "com.sos.jobscheduler.model.instruction.InstructionType",
      "default": "Job",
      "enum": [ "Job" ]
    },
    "jobPath": {
      "$ref": "../common/path-schema.json"
    },
    "agentPath": {
      "$ref": "../common/path-schema.json"
    },
    "returnCodeMeaning": {
    	"type": "object",
    	"javaType": "com.sos.jobscheduler.model.job.JobReturnCode",
    	"properties": {
    	  "success": {
      		"type": "array",
      		"items": {
        	  "$ref": "../common/nonNegativeInteger-schema.json"
        	}
      	  },
      	  "failure": {
      		"type": "array",
      		"items": {
        	  "$ref": "../common/nonNegativeInteger-schema.json"
        	}
      	  }
    	},
    	"additionalProperties": false
    }
  },
  "additionalProperties": false
}
