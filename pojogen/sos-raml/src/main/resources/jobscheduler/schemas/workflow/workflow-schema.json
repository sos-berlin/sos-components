{
  "title": "workflow",
  "id": "schemas/workflow/workflow",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.jobscheduler.model.workflow.Workflow",
  "type": "object",
  "required": [
    "path",
    "instructions"
  ],
  "properties": {
    "path": {
	  "$ref": "../common/path-schema.json"
	},
	"versionId": {
	  "type": "string"
	},
	"TYPE": {
	  "type": "string",
	  "default": "Workflow"
	},
    "instructions": {
      "type": "array",
      "items": {
      	"$ref": "../instruction/instructions-schema.json"
      }
    },
    "jobs": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "javaType": "com.sos.jobscheduler.model.job.Job"
      }
    }
  },
  "additionalProperties": false
}