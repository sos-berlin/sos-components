{
	"TYPE": "Workflow",
	"path": "my_workflow",
	"orderRequirements": {
		"parameters": {
			"myString": {
				"type": "String",
				"default": "DEFAULT"
			},
			"myNumber": {
				"type": "Number"
			}
		}
	},
	"instructions": [
		{
			"TYPE": "Execute.Named",
			"jobName": "job_first",
			"label": "job_first label"
		},
		{
			"TYPE": "Fork",
			"branches": [
				{
					"id": "branch1",
					"workflow": {
						"instructions": [
							{
								"TYPE": "Lock",
								"lockId": "lock_1",
								"count": 1,
								"lockedWorkflow": {
									"instructions": [
										{
											"TYPE": "Execute.Named",
											"jobName": "job_in_lock",
											"label": "job_in_lock label"
										}
									]
								}
							},
							{
								"TYPE": "Execute.Named",
								"jobName": "job_after_lock",
								"label": "job_after_lock label"
							}
						]
					}
				},
				{
					"id": "branch2",
					"workflow": {
						"instructions": [
							{
								"TYPE": "If",
								"predicate": "returnCode > 0",
								"then": {
									"instructions": [
										{
											"TYPE": "Execute.Named",
											"jobName": "job_2",
											"label": "job_2 label"
										}
									]
								}
							}
						]
					}
				}
			]
		},
		{
			"TYPE": "Execute.Named",
			"jobName": "job_last",
			"label": "job_last label"
		}
	],
	"jobs": {
		"job_1": {
			"agentId": "agent-1",
			"executable": {
				"TYPE": "ShellScriptExecutable",
				"script": "job_1 script"
			},
			"defaultArguments": {
				"job_1_arg_1": "job_1_arg_1 value",
				"job_1_arg_2": "job_1_arg_2 value"
			},
			"returnCodeMeaning": {
				"success": [
					0,
					5
				]
			},
			"taskLimit": 1
		},
		"job_2": {
			"agentId": "agent-1",
			"executable": {
				"TYPE": "ShellScriptExecutable",
				"script": "job_2 script"
			},
			"defaultArguments": {
				"job_2_arg_1": true,
				"job_2_arg_2": 1,
				"job_2_arg_3": 1.50
			},
			"returnCodeMeaning": {
				"success": [
					0
				]
			},
			"taskLimit": 1
		},
		"job_2_1_unused": {
			"agentId": "agent-1",
			"executable": {
				"TYPE": "ShellScriptExecutable",
				"script": "job_2 script"
			},
			"returnCodeMeaning": {
				"success": [
					0
				]
			},
			"taskLimit": 1
		},
		"job_3_unused": {
			"agentId": "agent-2",
			"executable": {
				"TYPE": "ShellScriptExecutable",
				"script": "job_3 script"
			},
			"returnCodeMeaning": {
				"success": [
					0
				]
			},
			"taskLimit": 1
		}
	}
}