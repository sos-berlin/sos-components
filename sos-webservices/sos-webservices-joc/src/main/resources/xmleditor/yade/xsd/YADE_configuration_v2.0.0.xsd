				        <xs:element ref="CredentialStoreFragmentRef" minOccurs="0"/>
		        
                <xs:element ref="BasicConnection"/>
				        <xs:element ref="SSHAuthentication"/>
                
                <xs:element ref="YADEClientCommand"/>
                <xs:element name="TempDirectoryParent" minOccurs="0" default="/tmp">
                    <xs:annotation>
                  <xs:appinfo source="Fragments/ProtocolFragments/JUMP/TempDirectoryParent"/>
                  <xs:documentation>
                     <div xmlns="http://www.w3.org/1999/xhtml">
                        <strong>References</strong>
                        <ul>
                           <li>
                              <a target="_blank" href="https://kb.sos-berlin.com/x/fI12B">JS7 - File Transfer Configuration</a>
                           </li>
                        </ul>
                        <strong>Notes</strong>
                        <p>Parent directory on the jump host used to create exactly one temporary subdirectory per file transfer operation (e.g.<em>yade-dmz-&lt;uuid&gt;</em>).</p>
                        <p>This subdirectory is deleted in all cases — whether the transfer succeeds or fails.</p>
                        <p>The parent directory itself remains.</p>
                     </div>
                  </xs:documentation>
               </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>	            
                </xs:element>
                
                <xs:element ref="ProxyForSFTP" minOccurs="0"/>
				
                <xs:element ref="SocketTimeout" minOccurs="0"/>
                <xs:element name="KeepAlive" type="SFTPKeepAliveType" minOccurs="0"/>
                <xs:element ref="StrictHostkeyChecking" minOccurs="0"/>
				        <xs:element ref="ConfigurationFiles" minOccurs="0"/>	
				
                <xs:element ref="SFTPPreProcessing" minOccurs="0"/>
				        <xs:element ref="SFTPPostProcessing" minOccurs="0"/>
                <xs:element ref="ProcessingCommandDelimiter" minOccurs="0"/>
                
				        <xs:element ref="Platform" minOccurs="0"/>
			      </xs:sequence>
			      <xs:attribute name="name" type="xs:NMTOKEN" use="required">
            <xs:annotation>
               <xs:documentation>
                  <div xmlns="http://www.w3.org/1999/xhtml">
                     <p>
This attribute identifies the current fragment. Any number of fragments can be used and are distinguished by YADE based on their name.
</p>
                     <p>
A fragment is referenced via this attribute value by a transfer profile.
</p>
                  </div>
               </xs:documentation>
            </xs:annotation>
                
            </xs:attribute>
		    </xs:complexType>
