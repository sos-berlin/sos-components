         <xs:appinfo source="Fragments/NotificationFragments/MailFragment/From"/>
         <xs:documentation>
            <div xmlns="http://www.w3.org/1999/xhtml">
               <strong>References</strong>
               <ul>
                  <li>
                     <a target="_blank" href="https://kb.sos-berlin.com/x/fI12B">JS7 - File Transfer Configuration</a>
                  </li>
               </ul>
               <strong>Notes</strong>
               <p>
			E-mail address of the account that sends e-mail.
		</p>
            </div>
         </xs:documentation>
      </xs:annotation>
		    <xs:simpleType>
			      <xs:restriction base="xs:string">
				        <xs:minLength value="1"/>
			      </xs:restriction>
		    </xs:simpleType>
	  </xs:element>
	  <xs:element name="To">
		    <xs:annotation>
         <xs:appinfo source="Fragments/NotificationFragments/MailFragment/To"/>
         <xs:documentation>
            <div xmlns="http://www.w3.org/1999/xhtml">
               <strong>References</strong>
               <ul>
                  <li>
                     <a target="_blank" href="https://kb.sos-berlin.com/x/fI12B">JS7 - File Transfer Configuration</a>
                  </li>
               </ul>
               <strong>Notes</strong>
               <p>
			E-mail address of the recipient(s) of a notification e-mail.
		</p>
            </div>
         </xs:documentation>
      </xs:annotation>
		    <xs:simpleType>
			      <xs:restriction base="xs:string">
				        <xs:minLength value="1"/>
			      </xs:restriction>
		    </xs:simpleType>
	  </xs:element>
	  <xs:element name="CC">
		    <xs:annotation>
         <xs:appinfo source="Fragments/NotificationFragments/MailFragment/CC"/>
         <xs:documentation>
            <div xmlns="http://www.w3.org/1999/xhtml">
               <strong>References</strong>
               <ul>
                  <li>
                     <a target="_blank" href="https://kb.sos-berlin.com/x/fI12B">JS7 - File Transfer Configuration</a>
                  </li>
               </ul>
               <strong>Notes</strong>
               <p>
			E-mail address of the recipient(s) of a carbon copy notification e-mail.
		</p>
            </div>
         </xs:documentation>
      </xs:annotation>
		    <xs:simpleType>
			      <xs:restriction base="xs:string">
				        <xs:minLength value="1"/>
			      </xs:restriction>
		    </xs:simpleType>
	  </xs:element>
