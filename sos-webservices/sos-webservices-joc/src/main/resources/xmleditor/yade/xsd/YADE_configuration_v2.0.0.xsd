            </div>
         </xs:documentation>
      </xs:annotation>
		    <xs:simpleType>
			      <xs:restriction base="xs:string">
				        <xs:minLength value="1"/>
			      </xs:restriction>
		    </xs:simpleType>
	  </xs:element>
	  <xs:element name="KeyStorePassword">
		    <xs:annotation>
         <xs:appinfo source="Fragments/ProtocolFragments/common/SSL/TrustedSSL/KeyStore/KeyStorePassword">
				        <XmlEditor type="password"/>
