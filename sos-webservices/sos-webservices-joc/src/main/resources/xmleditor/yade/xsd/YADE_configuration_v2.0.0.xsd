	  <xs:element name="ProxyForFTPS">
		    <xs:annotation>
         <xs:appinfo source="Fragments/ProtocolFragments/FTP/ProxyForFTPS"/>
         <xs:documentation>
            <div xmlns="http://www.w3.org/1999/xhtml">
               <strong>References</strong>
               <ul>
                  <li>
                     <a target="_blank" href="https://kb.sos-berlin.com/x/fI12B">JS7 - File Transfer Configuration</a>
                  </li>
               </ul>
               <strong>Notes</strong>
               <p>ProxyForFTPS - will use the HTTP or SOCKS proxy</p>
            </div>
         </xs:documentation>
      </xs:annotation>
		    <xs:complexType>
			      <xs:choice>
				        <xs:element ref="HTTPProxy"/>
				        <xs:element ref="SOCKSProxy"/>
			      </xs:choice>
		    </xs:complexType>
