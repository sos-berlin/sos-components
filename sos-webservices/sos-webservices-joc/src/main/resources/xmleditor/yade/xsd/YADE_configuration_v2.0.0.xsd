	  <xs:element name="TransferMode" default="binary">
		    <xs:annotation>
         <xs:appinfo source="Fragments/ProtocolFragments/FTP/TransferMode"/>
         <xs:documentation>
            <div xmlns="http://www.w3.org/1999/xhtml">
               <strong>References</strong>
               <ul>
                  <li>
                     <a target="_blank" href="https://kb.sos-berlin.com/x/fI12B">JS7 - File Transfer Configuration</a>
                  </li>
                  <li>
                     <a target="_blank"
                        href="https://kb.sos-berlin.com/display/PKB/YADE+Parameter+Reference+-+TransferMode">TransferMode</a>
