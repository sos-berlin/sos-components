<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] (%F:%L) - %m%n" />
		</Console>

	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="console" />
		</Root>
		<Logger name="com.sos" level="debug" />
		<Logger name="com.sos.joc.publish" level="info" />
	</Loggers>
</Configuration>